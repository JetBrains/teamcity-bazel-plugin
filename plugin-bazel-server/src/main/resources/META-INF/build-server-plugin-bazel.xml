<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
    <bean class="jetbrains.buildServer.bazel.BazelRunnerRunType"/>
    <bean class="jetbrains.buildServer.bazel.BazelRunnerDiscoveryExtension"/>
    <bean class="jetbrains.buildServer.bazel.fetchers.BazelCommandFetcher"/>
    <bean class="jetbrains.buildServer.bazel.fetchers.BazelTargetFetcher"/>
</beans>